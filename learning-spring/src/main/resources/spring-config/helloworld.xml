<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
    <!--<context:component-scan base-package="com.wangfengbabe.learning"/>-->
    <!--<context:annotation-config/>-->

    <bean id="helloworld" class="com.wangfengbabe.learning.helloword.HelloWorld">
        <constructor-arg index="0" value="constructor inject"></constructor-arg>
    </bean>

    <bean id="helloworld2" class="com.wangfengbabe.learning.helloword.HelloWorld">
        <property name="message" value="setter inject"/>
